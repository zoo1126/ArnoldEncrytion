<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="Menutop" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller.MenutopController">
   <children>
      <MenuBar id="menu">
        <menus>
          <Menu fx:id="file" mnemonicParsing="false" onAction="#fileevent" text="文件">
               <items>
                  <MenuItem id="open" fx:id="open" mnemonicParsing="false" text="打开" />
                  <MenuItem id="close" fx:id="close" mnemonicParsing="false" text="关闭" />
               </items>
          </Menu>
          <Menu fx:id="manage" mnemonicParsing="false" onAction="#getmanage" text="编辑">
               <items>
                  <MenuItem fx:id="all" mnemonicParsing="false" text="选择全部" />
                  <MenuItem fx:id="noall" mnemonicParsing="false" text="取消选择全部" />
               </items>
          </Menu>
          <Menu fx:id="help" mnemonicParsing="false" onAction="#gethelp" text="帮助">
            <items>
                  <MenuItem id="usage" fx:id="usage" mnemonicParsing="false" text="使用方法" />
              <MenuItem id="aboutus" fx:id="aboutus" mnemonicParsing="false" text="关于我们" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </children>
</AnchorPane>
